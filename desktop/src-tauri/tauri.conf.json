{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Agin TV",
  "version": "0.1.0",
  "identifier": "rocks.agin.tv.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "https://localhost:5173",
    "frontendDist": "../dist"
  },
  "plugins": {
    "shell": {
      "open": true
    }
  },
  "app": {
    "windows": [
      {
        "title": "Agin TV",
        "width": 1422,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },

  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "externalBin": [],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "",
    "longDescription": "",
    "createUpdaterArtifacts": false,
    "publisher": "",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "",
      "exceptionDomain": "",
      "signingIdentity": null,
      "providerShortName": null,
      "entitlements": null
    },
    "iOS": {},
    "android": {},
    "linux": {
      "deb": {
        "depends": []
      }
    }
  }
}
